if [ -f "/home/sebastian/Documents/academia-collaboration/simics-project/.package-list" ]; then
    exec "/home/sebastian/Documents/arqui-proyecto-III/simics-6.0.166/bin/checkpoint-merge" --package-list "/home/sebastian/Documents/academia-collaboration/simics-project/.package-list" --project "/home/sebastian/Documents/academia-collaboration/simics-project" "$@"
else
    exec "/home/sebastian/Documents/arqui-proyecto-III/simics-6.0.166/bin/checkpoint-merge" --project "/home/sebastian/Documents/academia-collaboration/simics-project" "$@"
fi
